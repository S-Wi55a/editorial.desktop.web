{
  "compilerOptions": {
    "outDir":"./CompiledTypeScript",
    "sourceMap": true,
    "noImplicitAny": true,
    "target": "es6",
    "module": "es6",
    "jsx": "react",
    "allowJs": true
    //"diagnostics": true
  },
  "include": [
    "Features/**/*"
  ],
  "exclude": [
      "node_modules",
      "bower_components", 
      "jspm_packages",
      "**/*.scss",
      "./CompiledTypeScript"
  ]
}
