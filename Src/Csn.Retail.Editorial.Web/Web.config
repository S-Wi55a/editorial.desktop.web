<configuration>
  <configSections>
    <section name="TenantsSettings" type="Csn.MultiTenant.Configs.TenantConfigSection, Csn.MultiTenant" />
    <section name="CacheSettings" type="Csn.Cars.Cache.Configs.CacheSettingsSection, Csn.Cars.Cache" />
    <section name="CircuitBreakerSettings" type="Csn.Hystrix.Configs.CircuitConfigSection,Csn.Hystrix" />
    <section name="HystrixRestClientSettings" type="Csn.Hystrix.RestClient.Configs.HystrixRestClientSetting, Csn.Hystrix.RestClient" />
  </configSections>
  
  <TenantsSettings DefaultTenant="carsales">
    <Tenant Name="carsales">
      <add name="MediaMotiveAccountId" value="cs.allcars" />
      <add name="MediaMotiveUrl" value="//mm.carsales.com.au/carsales/" />
      <add name="SiteNavPath" value="navigation/carsales" />
    </Tenant>
    <Tenant Name="bikesales">
      <add name="MediaMotiveAccountId" value="cs.allcars" />
      <add name="MediaMotiveUrl" value="//mm.carsales.com.au/bikesales/" />
    </Tenant>
  </TenantsSettings>
  <CircuitBreakerSettings Disabled="False" DefaultHeartBeatInMs="10000" DefaultHistoryBufferSize="100" DefaultTimeoutThreshold="5" DefaultFailureThreshold="5" DefaultQueueLengthThreshold="1000" DefaultThreadDurationTimeoutInMs="1000" DefaultThreadExecutionTimeoutInMs="900" DefaultThreadAbortTimeoutInMs="2000" DefaultNumThreads="20">
    <Circuits>
      <Circuit Name="SiteNavApiProxy" HeartBeatInMs="30000" LatencyThresholdInMs="5000" QueueLengthThreshold="1000" ThreadDurationTimeoutInMs="3000" ThreadExecutionTimeoutInMs="2000" ThreadAbortTimeoutInMs="4000" />
      <Circuit Name="EditorialApiProxy" HeartBeatInMs="30000" LatencyThresholdInMs="5000" QueueLengthThreshold="1000" ThreadDurationTimeoutInMs="3000" ThreadExecutionTimeoutInMs="2000" ThreadAbortTimeoutInMs="4000" />
    </Circuits>
  </CircuitBreakerSettings>
  <HystrixRestClientSettings>
    <Hosts>
      <Host Name="SiteNavApiProxy" BaseUrl="http://sitenav.carsalesnetwork.csuat.com.au/v2" TimeoutInMs="1000" RetryOnFailure="1" CircuitConfig="SiteNavApiProxy" />
      <Host Name="EditorialApiProxy" BaseUrl="http://editorial-ui.api.csdev.com.au" TimeoutInMs="5000" RetryOnFailure="3" CircuitConfig="EditorialApiProxy" />
    </Hosts>
  </HystrixRestClientSettings>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <add key="aspnet:UseTaskFriendlySynchronizationContext" value="true" />
    <!--<add key="AssetDomain" value="//s3-ap-southeast-2.amazonaws.com/carsales-web/"/>-->
  </appSettings>
  <system.web>
    <authentication mode="None" />
    <compilation debug="true" targetFramework="4.6.1" />
    <httpRuntime enableVersionHeader="false" targetFramework="4.6.1" />

    <customErrors mode="Off" defaultRedirect="/error/500">
