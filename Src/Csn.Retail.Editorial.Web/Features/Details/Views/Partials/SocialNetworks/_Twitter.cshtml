@model Csn.Retail.Editorial.Web.Features.Details.Models.SocialMetaData


<a class="share-social__link share-social__link--twitter share-social__link--popup"
   href="https://twitter.com/intent/tweet?text=@Model.Title&amp;url=@Model.Url"
   onclick="window.open(this.href, 'social--twitter','left=50,top=50,width=600,height=443,toolbar=0'); return false;"
   target="_blank" data-webm-clickvalue="twitter">
    <div class="share-social__link-icon icon-twitter"></div>
    <div class="share-social__link-text">Twitter</div>
</a>