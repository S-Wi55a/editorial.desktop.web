@model Csn.Retail.Editorial.Web.Features.Details.Models.SocialMetaData

<meta property="twitter:title" content="@Model.Title" />
<meta property="twitter:description" content="@Model.Description" />
<meta property="twitter:image:src" content="@Model.Image?size=thumb" />
<meta property="og:type" content="@Model.Type" />
<meta property="og:title" content="@Model.Title" />
<meta property="og:url" content="@Model.Url" />
<meta property="og:description" content="@Model.Description" />
<meta property="og:image" content="@Model.Image?height=700&amp;width=1050" />
<meta property="og:site_name" content="@Model.SiteName"/>