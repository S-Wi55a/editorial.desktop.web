@using Csn.Retail.Editorial.Web.Features.Shared.Proxies.EditorialApi
@model Csn.Retail.Editorial.Web.Features.Details.Models.HeroSection


@if (Model.Type == HeroType.MultipleImages)
{
    Html.RenderPartial("Partials/Hero/_MultipleImages", Model);
}
else if (Model.Type == HeroType.ImagesAndVideo)
{
    Html.RenderPartial("Partials/Hero/_ImagesAndVideo", Model);
}
else if (Model.Type == HeroType.Video)
{
    Html.RenderPartial("Partials/Hero/_SingleVideo", Model);
}
else if (Model.Type == HeroType.SingleImage)
{
    Html.RenderPartial("Partials/Hero/_SingleImage", Model);
}
else if (Model.Type == HeroType.DoubleImage)
{
    Html.RenderPartial("Partials/Hero/_DoubleImage", Model);
}
else if (Model.Type == HeroType.WideImage)
{
    Html.RenderPartial("Partials/Hero/_WideImage", Model);
}
