        "redirectInstruction": "/editorial/lexus/$2/$3",
        "includeQueryStringInRedirect": true,
        "description": "LexusMarketingGroupRedirect1"
      },
      {
        "ruleType": "regex",
        "name": "LexusMktgGroup2",
        "matchRule": "\\/editorial\\/lexus\\/(ct|gs|is|ls|lx|rx|sc|rc|lf|es|lc)-?(20|25|27|30|33|35|43|45|46|47|50|57|60)0-?(h|t|hl|d|c|f|nx|a)?\\/([^\\?]*\\/?)?$",
        "redirectInstruction": "/editorial/lexus/$1/$4",
        "includeQueryStringInRedirect": true,
        "description": "LexusMarketingGroupRedirect2"
      },
      {
        "ruleType": "regex",
        "name": "LexusMktgGroup3",
        "matchRule": "\\/editorial\\/lexus\\/m-marketing-group\\/([^\\?]*\\/?)?$",
        "redirectInstruction": "/editorial/lexus/ct/$1",
        "includeQueryStringInRedirect": true,
        "description": "LexusMarketingGroupRedirect3"
      },
      {
        "ruleType": "regex",
        "name": "LexusMktgGroup4",
        "matchRule": "\\/editorial\\/lexus\\/(is|rc)-(f|350-prestige)\\/([^\\?]*\\/?)?$",
        "redirectInstruction": "/editorial/lexus/$1/$3",
        "includeQueryStringInRedirect": true,
        "description": "LexusMarketingGroupRedirect4"
      },
      {
        "ruleType": "regex",
        "name": "LexusMktgGroup5",
        "matchRule": "\\/editorial\\/lexus\\/lf-nx\\/([^\\?]*\\/?)?$",
        "redirectInstruction": "/editorial/lexus/nx/$1",
        "includeQueryStringInRedirect": true,
        "description": "LexusMarketingGroupRedirect5"
      },
      {
        "ruleType": "regex",
        "name": "LexusMktgGroup6",
        "matchRule": "\\/editorial\\/lexus\\/lf-a\\/([^\\?]*\\/?)?$",
        "redirectInstruction": "/editorial/lexus/lfa/$1",
        "includeQueryStringInRedirect": true,
        "description": "LexusMarketingGroupRedirect6"
      },
      {
        "ruleType": "regex",
        "name": "LegacyDetailsCarsales",
        "matchRule": "\\/editorial\\/(news|reviews|advice|videos|features|riding-advice|tips|tow-tests|motoracing|engine-reviews|products)(\\/.*)?\\/(.*-\\d+)\\/?$",
        "redirectInstruction": "/editorial/details/$3/",
        "includeQueryStringInRedirect": true,
        "description": "Used for redirecting the legacy article details urls. We need to duplicate this year to prevent the next 2 redirects from hijacking the legacy details redirect"
      },
      {
        "ruleType": "regex",
        "name": "ArticleTypeIa1",
        "matchRule": "(.*?)\\/(news|advice)\\/([^\\?]*)",
        "redirectInstruction": "$1/car-$2/$3",
        "includeQueryStringInRedirect": true,
        "description": "Used for redirecting the old article type seo fragments generated by Ryvuss"
      },
      {
        "ruleType": "regex",
        "name": "ArticleTypeIa2",
        "matchRule": "(.*?)\\/((review|video|feature)s?)\\/([^\\?]*)",
        "redirectInstruction": "$1/car-$3s/$4",
        "includeQueryStringInRedirect": true,
        "description": "Used for redirecting the old article type seo fragments generated by Ryvuss"
      },
      {
        "ruleType": "regex",
        "name": "ArticleTypeIaTagRemoval1",
        "matchRule": "(.*?)\\/((news|advice)-article-type)\\/([^\\?]*)",
        "redirectInstruction": "$1/car-$3/$4",
        "includeQueryStringInRedirect": true,
        "description": "Used for redirecting the old article type seo fragments generated by Ryvuss"
      },
      {
        "ruleType": "regex",
        "name": "ArticleTypeIaTagRemoval2",
        "matchRule": "(.*?)\\/((review|video|feature)-article-type)\\/([^\\?]*)",
        "redirectInstruction": "$1/car-$3s/$4",
        "includeQueryStringInRedirect": true,
        "description": "Used for redirecting the old article type seo fragments generated by Ryvuss"
      },
      {
        "ruleType": "regex",
        "name": "CarsCategoryUpdate",
        "matchRule": "(.*?)\\/((4x4-offroad|electric|family|green|hybrid|performance|prestige|tradie)-category)\\/([^\\?]*)",
        "redirectInstruction": "$1/$3-cars-category/$4",
        "includeQueryStringInRedirect": true,
        "description": "Used for redirecting the old category urls to those with cars included"
      }
    ]
  }
}