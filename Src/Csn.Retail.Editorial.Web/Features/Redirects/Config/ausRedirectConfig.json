{
  "redirects": {
    "common": [
      {
        "ruleType": "regex",
        "name": "LegacyDetails",
        "matchRule": "\\/editorial\\/(news|reviews|advice|videos|features|riding-advice|tips|tow-tests|motoracing|engine-reviews|products)(\\/.*)?\\/(.*-\\d+)\\/?$",
        "redirectInstruction": "/editorial/details/$3/",
        "includeQueryStringInRedirect": true,
        "description": "Used for redirecting the legacy article details urls"
      },
      {
        "ruleType": "regex",
        "name": "ArticleTypePluralisationFix",
        "matchRule": "(.*?)\\/((review|video|feature)s)\\/([^\\?]*)",
        "redirectInstruction": "$1/$3/$4",
        "includeQueryStringInRedirect": true,
        "description": "To redirect the plural version of our article type pages to the singular version now supported by Ryvuss"
      },
      {
        "ruleType": "regex",
        "name": "ArticleTypeTagRemoval",
        "matchRule": "(.*?)\\/((.*?)-article-type)\\/([^\\?]*)",
        "redirectInstruction": "$1/$3/$4",
        "includeQueryStringInRedirect": true,
        "description": "Used for redirecting the old article type seo fragments generated by Ryvuss"
      }
    ],
    "redbook-cars": [
      {
        "ruleType": "regex",
        "name": "vertical",
        "matchRule": "\\/editorial\\/?$",
        "redirectInstruction": "/editorial/cars/",
        "includeQueryStringInRedirect": true,
        "description": "Used for redirecting the redbook default vertical"
      }
    ],
    "carsales": [
      {
        "ruleType": "regex",
        "name": "MercMktgGroup1",
        "matchRule": "\\/editorial\\/mercedes-benz\\/(.*\\/)?(.*)-class-marketing-group\\/([^\\?]*\\/?)?$",
        "redirectInstruction": "/editorial/mercedes-benz/$2-class/$3",
        "includeQueryStringInRedirect": true,
        "description": "MercMarketingGroupRedirect1"
      },
      {
        "ruleType": "regex",
        "name": "MercMktgGroup2",
        "matchRule": "\\/editorial\\/mercedes-benz\\/(.*)\\/vans-marketing-group\\/([^\\?]*\\/?)?$",
        "redirectInstruction": "/editorial/mercedes-benz/$1/$2",
        "includeQueryStringInRedirect": true,
        "description": "MercMarketingGroupRedirect2"
      },
      {
        "ruleType": "regex",
        "name": "MercMktgGroup3",
        "matchRule": "\\/editorial\\/mercedes-benz\\/vans-marketing-group\\/([^\\?]*\\/?)?$",
        "redirectInstruction": "/editorial/mercedes-benz/sprinter/$1",
        "includeQueryStringInRedirect": true,
        "description": "MercMarketingGroupRedirect3"
      },
      {
        "ruleType": "regex",
        "name": "BMWMktgGroup1",
        "matchRule": "\\/editorial\\/bmw\\/(.*\\/)?(1|2|3|4|5|6|7|8)-series-marketing-group\\/([^\\?]*\\/?)?$",
        "redirectInstruction": "/editorial/bmw/$2-series/$3",
        "includeQueryStringInRedirect": true,
        "description": "BMWMarketingGroupRedirect1"
      },
      {
        "ruleType": "regex",
