        "matchRule": "\\/editorial\\/bmw\\/(.*\\/)(i-series|x-models|m-models|z-models)-marketing-group\\/([^\\?]*\\/?)?$",
        "redirectInstruction": "/editorial/bmw/$1/$3",
        "includeQueryStringInRedirect": true,
        "description": "BMWMarketingGroupRedirect2"
      },
      {
        "ruleType": "regex",
        "name": "BMWMktgGroup3",
        "matchRule": "\\/editorial\\/bmw\\/x-models-marketing-group\\/([^\\?]*\\/?)?$",
        "redirectInstruction": "/editorial/bmw/x5/$1",
        "includeQueryStringInRedirect": true,
        "description": "BMWMarketingGroupRedirect3"
      },
      {
        "ruleType": "regex",
        "name": "BMWMktgGroup4",
        "matchRule": "\\/editorial\\/bmw\\/m-models-marketing-group\\/([^\\?]*\\/?)?$",
        "redirectInstruction": "/editorial/bmw/m3/$1",
        "includeQueryStringInRedirect": true,
        "description": "BMWMarketingGroupRedirect4"
