@model Csn.Retail.Editorial.Web.Features.Shared.Models.WebTrendData

<!-- Start of webtrends tracking tags -->
<script type="text/javascript">
	window.webtrendsAsyncInit=function(){
        var dcs=new Webtrends.dcs().init({
            dcsid:"@Model.WebTrendId",
            paidsearchparams:"gclid,mkwid",
            domain:"statse.webtrendslive.com",
            timezone:10,
            i18n:false,
            fpcdom:".@Model.DomainName",
            plugins:{
                webm:{src:"//js.csnstatic.com/lib/tracking/webm.wt.nontsrc.plugins.min.js"}
            }
        }).track({
            transform: function(tag, options) {
                //Includes WT.z_ep_vt on first hit of visit and again 10 / 60 seconds later
                if (tag.WT.vt_f_tlv != '') {
                    options['argsa'].push("WT.z_ep_vt", "0");
                    setTimeout(function(){Webtrends.multiTrack({
                        args: {"WT.dl":"65", "WT.z_ep_vt":"10"}
                    })}, 10000);
                    setTimeout(function(){Webtrends.multiTrack({
                        args: {"WT.dl":"65", "WT.z_ep_vt":"60"}
                    })}, 60000);
                }

                //Includes WT.site parameter
                options['argsa'].push("WT.site", document.location.hostname.replace('www.', ''));

                //Removes 'q' parameter from query string and referring URL
                tag.qre=new RegExp('q=[^\&]*\&?');
                if (tag.DCS.dcsqry) tag.DCS.dcsqry = tag.DCS.dcsqry.replace(tag.qre,'');
                if (tag.DCS.dcsref && (document.referrer && document.referrer.split('/').length > 2 && (document.referrer.split('/')[2] == window.location.hostname))) tag.DCS.dcsref = tag.DCS.dcsref.replace(tag.qre,'');

            }, finish: function(tag, options) {}
        });
    };
    webmIsBrandRelated = false;
    (function(){
        var s=document.createElement("script"); s.async=true; s.src="//js.csnstatic.com/lib/tracking/webtrends_custom.min.js";
        var s2=document.getElementsByTagName("script")[0]; s2.parentNode.insertBefore(s,s2);
    }());
</script>
<noscript><img alt="dcsimg" id="dcsimg" width="1" height="1" src="//statse.webtrendslive.com/@Model.WebTrendId/njs.gif?dcsuri=/nojavascript&amp;WT.js=No&amp;WT.tv=10.4.16&amp;dcssip=.@Model.DomainName" /></noscript>

<!-- end of tracking tags -->