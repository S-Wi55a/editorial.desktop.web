@using React.Web.Mvc
@model Csn.Retail.Editorial.Web.Infrastructure.React.Models.ReduxStoreModel

@*

    Store - you have to manually request what part of the store you want b/c there is no guartee the data will be there from the components rendered earlier
    
    ReducerKey - Should be top level key in state tree
    
    ReducerName - Maps to the reducer collections, should be a top level reducer
        
*@


@Html.React("global.Components.PartOfStore", new
{
    reducerKey = Model.ReducerKey,
    reducerName = Model.ReducerName,
    state = Model.State
})