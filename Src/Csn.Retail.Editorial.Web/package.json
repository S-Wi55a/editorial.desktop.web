    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "postinstall": "npm rebuild node-sass --force && npm run bower install",
        "webpack": "webpack",
        "webpack-dev-server": "node node_modules/webpack-dev-server/bin/webpack-dev-server.js",
        "bower": "bower",
        "babel": "babel",
        "rimraf": "rimraf",
        "cross-env": "cross-env",
        "transform-webpack": "npm run rimraf TempWebpackConfig && npm run babel -- WebpackConfig -d TempWebpackConfig",
        "build-client": "npm run transform-webpack && cross-env NODE_ENV=development npm run webpack -- --color --config  TempWebpackConfig/Client/webpack.client.config.js",
        "build-server": "npm run transform-webpack && cross-env NODE_ENV=development npm run webpack -- --color --config  TempWebpackConfig/Server/webpack.server.config.js",
        "build": "npm run transform-webpack && cross-env NODE_ENV=development cross-env TENANT=&& npm run webpack -- --color --config  TempWebpackConfig/webpack.isomorphic.config.js ",
        "build-custom": "npm run transform-webpack && cross-env NODE_ENV=development npm run webpack -- --color --config  TempWebpackConfig/webpack.isomorphic.config.js ",
        "build-release": "npm run transform-webpack && cross-env NODE_ENV=production cross-env TENANT= npm run webpack -- -p --color --config  TempWebpackConfig/webpack.isomorphic.config.js ",
        "build-release-custom": "npm run transform-webpack && cross-env NODE_ENV=production npm run webpack -- -p --color --config  TempWebpackConfig/webpack.isomorphic.config.js ",
        "dev-watch": "npm run transform-webpack && cross-env NODE_ENV=development npm run webpack -- --progress --colors --watch --config  TempWebpackConfig/webpack.isomorphic.config.js ",
        "dev-watch-client": "cross-env NODE_ENV=development npm run webpack -- --progress --colors --watch --config  TempWebpackConfig/Client/webpack.client.config.js ",
        "dev-watch-server": "cross-env NODE_ENV=development npm run webpack -- --progress --colors --watch --config  TempWebpackConfig/Server/webpack.server.config.js ",
        "dev": "npm run transform-webpack && cross-env NODE_ENV=development concurrently -k \"npm run dev-watch-server\" \"npm run webpack-dev-server -- --colors --hot --watch --config  TempWebpackConfig/Client/webpack.client.config.js\" "
    },
